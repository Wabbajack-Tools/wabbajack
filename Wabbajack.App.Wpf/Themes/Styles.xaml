                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="Bg" Property="Background" Value="{StaticResource ScrollBarDisabledBackground}" />
                        </Trigger>
                        <Trigger SourceName="PART_Thumb" Property="IsMouseOver" Value="True">
                            <Setter TargetName="PART_TrackBg" Property="Fill" Value="{StaticResource VerticalScrollBarBackground}" />
